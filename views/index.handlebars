<body>
    <header>
        {{!-- carousel for categories --}}
       <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
                <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner">
                <div class="carousel-item active">
                <img src="/images/movie.jpg" class="d-block w-100" alt="...">
                </div>
                <div class="carousel-item">
                <img src="/images/history.jpg" class="d-block w-100" alt="...">
                </div>
                <div class="carousel-item">
                <img src="..." class="d-block w-100" alt="...">
                </div>
            </div>
            <button class="carousel-control-prev" type="button" data-target="#carouselExampleIndicators" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-target="#carouselExampleIndicators" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </button>
        </div>
</div>
            
    </header>
    <div class="container-md">
        <div class="card-deck">
            <div class="card" style="width: 18rem">
                <img src="/images/movie.jpg" class="card-img-top img-fluid" alt="image">
                <div class="card-body">
                    <h5 class="card-title">Quiz 1</h5>
                    <p class="card-text">A fun quiz about the captials of the world!</p>
                    <a href="#" class="btn btn-primary">Play</a>
                </div>
            </div>
            <div class="card" style="width: 18rem">
                <img src="/images/history.jpg" class="card-img-top img-fluid" alt="image">
                <div class="card-body">
                    <h5 class="card-title">Quiz 2</h5>
                    <p class="card-text">Test your JavaScript knowledge</p>
                    <a href="#" class="btn btn-primary">Play</a>
                </div>
            </div>
            <div class="card" style="width: 18rem">
                <img src="..." class="card-img-top img-fluid" alt="image">
                <div class="card-body">
                    <h5 class="card-title">Quiz 3</h5>
                    <p class="card-text">Movies, movies, movies!</p>
                    <a href="#" class="btn btn-primary">Play</a>
                </div>
            </div>
        </div>
    </div>    
</body>
